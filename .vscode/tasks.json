    "version": "2.0.0",
    "tasks": [
        {
            "label": "Clio2JVC-build",
            "type": "shell",
            "command": "C:/Arduino-CLI/arduino-cli.exe compile -v -b arduino:avr:leonardo Clio2JVC.ino",
            "group": "build",
            "isBackground": false,
            "presentation": {
                "echo": true,
                "reveal": "always",
                "focus": false,
                "panel": "shared",
                "showReuseMessage": true
            },
            "problemMatcher": []
        },
        {
            "label": "Clio2JVC-upload",
            "type": "shell",
            "command": "C:/Arduino-CLI/arduino-cli.exe upload -p COM18 -v --fqbn arduino:avr:leonardo Clio2JVC.ino",
            "group": "test",
            "isBackground": false,
            "presentation": {
                "echo": true,
                "reveal": "always",
                "focus": false,
                "panel": "shared",
                "showReuseMessage": true
            },
            "problemMatcher": []
        }
    ]
}
